@model IEnumerable<Chat>

<div class="side-menu">
    @foreach (var chat in Model)
    {
        <a class="room-button" asp-controller="Home" asp-action="Chat" asp-route-id="@chat.Id" asp-route-roomName="@chat.Name">
            @chat.Name
        </a>
    }

    <a class="room-button" id="create-room-btn" href="#">
        <strong>+</strong>
    </a>
</div>

<script src="~/lib/jquery/dist/jquery.js"></script>
